<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="AppBaseTheme" parent="@android:style/Theme.Light" />

    <style name="AppTheme" parent="@style/AppBaseTheme" />

    <style name="equize_seekbar">
        <item name="android:paddingLeft">@dimen/vertical_seedbar_padding</item>
        <item name="android:paddingRight">@dimen/vertical_seedbar_padding</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:maxHeight">8.0dip</item>
        <item name="android:progressDrawable">@drawable/seekbar_img</item>
        <item name="android:minHeight">8.0dip</item>
        <item name="android:thumb">@drawable/equilizer_seekbar_thumbvol</item>
        <item name="android:thumbOffset">20.0dip</item>
        <item name="android:layout_centerHorizontal">true</item>
    </style>
</resources>
